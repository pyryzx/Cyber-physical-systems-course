#define F_CPU 16000000UL
#include <avr/io.h>
#include <avr/interrupt.h>

ISR(TIMER1_COMPA_vect) 
    PORTB ^= (1 << PB5);     
}

int main(void) {
    DDRB |= (1 << PB5);       

    TCCR1B = (1 << WGM12) | (1 << CS12) | (1 << CS10); 
    OCR1A  = 15625;          
    TIMSK1 = (1 << OCIE1A);    
    sei();

    while (1);
}